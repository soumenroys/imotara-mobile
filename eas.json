{
    "cli": {
        "version": ">= 10.0.0",
        "appVersionSource": "local"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "android": {
                "buildType": "apk"
            },
            "ios": {
                "simulator": true
            }
        },
        "preview": {
            "distribution": "internal",
            "android": {
                "buildType": "apk"
            },
            "ios": {
                "simulator": false
            },
            "env": {
                "EXPO_PUBLIC_IMOTARA_API_BASE_URL": "https://imotaraapp.vercel.app",
                "EXPO_PUBLIC_LAUNCH_CLOUD_SYNC_FREE_FOR_ALL": "true"
            }
        },
        "iosTestflight": {
            "distribution": "store",
            "ios": {
                "simulator": false
            },
            "env": {
                "EXPO_PUBLIC_IMOTARA_API_BASE_URL": "https://imotaraapp.vercel.app",
                "EXPO_PUBLIC_LAUNCH_CLOUD_SYNC_FREE_FOR_ALL": "true"
            }
        },
        "production": {
            "distribution": "store",
            "android": {
                "buildType": "app-bundle"
            },
            "ios": {
                "simulator": false
            },
            "env": {
                "EXPO_PUBLIC_IMOTARA_API_BASE_URL": "https://imotaraapp.vercel.app",
                "EXPO_PUBLIC_LAUNCH_CLOUD_SYNC_FREE_FOR_ALL": "true"
            }
        }
    },
    "submit": {
        "iosTestflight": {
            "ios": {
                "appleId": "roysoumen@icloud.com",
                "ascAppId": "128384266",
                "appleTeamId": "MY64TKZ69W"
            }
        },
        "production": {}
    }
}