{
    "cli": {
        "version": ">= 10.0.0"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "android": {
                "buildType": "apk"
            },
            "ios": {
                "simulator": true
            }
        },
        "preview": {
            "distribution": "internal",
            "android": {
                "buildType": "apk"
            },
            "ios": {
                "simulator": false
            },
            "env": {
                "EXPO_PUBLIC_IMOTARA_API_BASE_URL": "https://imotaraapp.vercel.app"
            }
        },
        "production": {
            "distribution": "store",
            "android": {
                "buildType": "app-bundle"
            },
            "ios": {
                "simulator": false
            },
            "env": {
                "EXPO_PUBLIC_IMOTARA_API_BASE_URL": "https://imotaraapp.vercel.app"
            }
        }
    },
    "submit": {
        "production": {}
    }
}